use Tk;
#use Tcl::pTk;
my $mw = MainWindow->new;
my $tw = $mw->Text()->pack;

my $t0=time;

for my $stresser ('a'..'zzz') {
  $tw->windowCreate('end',-window=>$tw->Button(-text=>$stresser));
  if ($stresser =~ /z$/) {
    $tw->insert('end',"\n") ;
    $tw->see('end');
    #$tw->update;
  }
}

my $t1=time;
print STDERR "time=".($t1-$t0)."\n";

#MainLoop;

